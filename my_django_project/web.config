<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <system.webServer>
        <handlers>
            <add name="Python FastCGI" path="*" verb="*" modules="FastCgiModule" scriptProcessor="C:\Python\python.exe|C:\Path\to\wfastcgi.py" resourceType="Unspecified" requireAccess="Script" />
        </handlers>
    </system.webServer>
    <appSettings>
        <add key="WSGI_HANDLER" value="my_django_project.wsgi.application" />
        <add key="PYTHONPATH" value="C:\inetpub\wwwroot\my_django_project" />
        <add key="DJANGO_SETTINGS_MODULE" value="my_django_project.settings.prod" />
    </appSettings>
</configuration>